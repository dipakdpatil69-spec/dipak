NODE_ENV=development
PORT=3001
WEBSOCKET_PORT=8080

# Security Configuration
ENABLE_REAL_MONITORING=true
SCAN_INTERVAL=30000
LOG_LEVEL=info

# Network Configuration
NETWORK_SCAN_RANGE=auto
ENABLE_NETWORK_MONITORING=true
ENABLE_LOG_MONITORING=true
ENABLE_THREAT_DETECTION=true

# API Configuration
API_RATE_LIMIT=100
ENABLE_CORS=true